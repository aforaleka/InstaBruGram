<%= form_for @photo do |f| %>
  <div class="form-group">
    <%= f.label "image url" %><br>
    <%= f.text_field :image, class: "form-control" %>
  </p>

  <p class="form-group">
    <%= f.label :caption %><br>
    <%= f.text_field :caption, class: "form-control"  %>
  </p>

  <p class="form-group">
    <%= f.label "privacy" %><br>
    <%= f.select :public, [['Public', true], ['Private', false]], class: "form-control"  %>
  </p>

  <p>
    <%= f.submit class: "btn btn-default" %>
  </p>

<% end %>
